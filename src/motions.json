{
  "schemaVersion": 1,
  "settings": {
    "allowRegisterPrefix": true
  },
  "operators": [
    { "id": "delete", "keys": ["d"], "appliesTo": ["motion", "textobj"], "supportsCount": true },
    { "id": "change", "keys": ["c"], "appliesTo": ["motion", "textobj"], "supportsCount": true },
    { "id": "yank",   "keys": ["y"], "appliesTo": ["motion", "textobj"], "supportsCount": true },
    { "id": "indent", "keys": [">"], "appliesTo": ["motion", "textobj"], "supportsCount": true },
    { "id": "dedent", "keys": ["<"], "appliesTo": ["motion", "textobj"], "supportsCount": true },
    { "id": "reindent", "keys": ["="], "appliesTo": ["motion", "textobj"], "supportsCount": true },
    { "id": "toggle_case", "keys": ["g","~"], "appliesTo": ["motion","textobj"], "supportsCount": true },
    { "id": "lowercase",   "keys": ["g","u"], "appliesTo": ["motion","textobj"], "supportsCount": true },
    { "id": "uppercase",   "keys": ["g","U"], "appliesTo": ["motion","textobj"], "supportsCount": true },
    { "id": "reflow", "keys": ["g","w"], "appliesTo": ["motion","textobj"], "supportsCount": true }
  ],
  "operatorSelf": [
    { "keys": ["d","d"], "operator": "delete", "target": { "type": "line" } },
    { "keys": ["y","y"], "operator": "yank",   "target": { "type": "line" } },
    { "keys": ["c","c"], "operator": "change", "target": { "type": "line" } },
    { "keys": [">",">"], "operator": "indent", "target": { "type": "line" } },
    { "keys": ["<","<"], "operator": "dedent", "target": { "type": "line" } },
    { "keys": ["=","="], "operator": "reindent", "target": { "type": "line" } }
  ],
  "motions": [
    { "id": "left",  "keys": ["h"], "acceptsCount": true },
    { "id": "down",  "keys": ["j"], "acceptsCount": true },
    { "id": "up",    "keys": ["k"], "acceptsCount": true },
    { "id": "right", "keys": ["l"], "acceptsCount": true },
    { "id": "display_down", "keys": ["g","j"], "acceptsCount": true },
