    { "id": "i`", "keys": ["i","`"], "type": "quote_inner", "delims": ["`","`"] },
    { "id": "a`", "keys": ["a","`"], "type": "quote_around", "delims": ["`","`"] },
    { "id": "ip", "keys": ["i","p"], "type": "paragraph_inner" },
    { "id": "ap", "keys": ["a","p"], "type": "paragraph_around" },
    { "id": "is", "keys": ["i","s"], "type": "sentence_inner" },
    { "id": "as", "keys": ["a","s"], "type": "sentence_around" }
