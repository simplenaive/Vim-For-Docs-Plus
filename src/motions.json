    { "id": "aw", "keys": ["a","w"], "type": "word_around" },
    { "id": "iW", "keys": ["i","W"], "type": "WORD" },
    { "id": "aW", "keys": ["a","W"], "type": "WORD_around" },
    { "id": "i(", "keys": ["i","("], "type": "paren_inner",  "delims": ["(",")"] },
