{
  "coreCommands": {
    "normal": {
      "v": "visualMode",
      "V": "visualLineMode", 
      ".": "repeatLastMotion",
      "j": "moveDown",
      "k": "moveUp",
      "s": "substituteChar"
    },
    "visual": {
      "Escape": "exitVisualMode",
      "j": "moveDown",
      "k": "moveUp"
    },
    "visualLine": {
      "Escape": "exitVisualMode",
      "j": "moveDown",
      "k": "moveUp"
    },
    "insert": {
      "Escape": "exitInsertMode",
      "ctrl+o": "tempNormalMode"
    }
  },
  
  "commands": {
    "paste": {
      "key": "p",
      "modes": ["normal", "visual", "visualLine"],
      "actions": [
        {"motion": "sendKeyEvent", "args": {"key": "right"}},
        {"motion": "clickMenu", "args": {"item": "paste"}},
        {"motion": "resetBuffer"}
      ],
      "repeatable": true,
      "supportsCount": true
    },
    
    "pasteBefore": {
      "key": "P", 
      "modes": ["normal"],
      "actions": [
        {"motion": "clickMenu", "args": {"item": "paste"}},
        {"motion": "resetBuffer"}
      ],
      "repeatable": true,
      "supportsCount": true
    },
    
    "undo": {
      "key": "u",
      "modes": ["normal", "visual", "visualLine"],
      "actions": [
        {"motion": "clickMenu", "args": {"item": "undo"}},
        {"motion": "resetBuffer"}
      ],
      "repeatable": true,
      "supportsCount": true
    },
    
    "redo": {
      "key": "r",
      "modes": ["normal", "visual", "visualLine"], 
      "actions": [
